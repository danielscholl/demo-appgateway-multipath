        "type": "Microsoft.Web/serverfarms",
        "kind": "app",
        "location": "eastus",
        "sku": {
            "name": "B1"
        },
        "properties": {}
      },
      {
        "apiVersion": "2016-08-01",
        "type": "Microsoft.Web/sites",
        "name": "[variables('westWeb')]",
        "dependsOn": [
            "[variables('westPlan')]"
        ],
        "location": "westus",
        "properties": {
            "name": "[variables('westWeb')]",
            "serverFarmId": "[variables('westPlan')]"
        },
        "resources": [
          {
            "apiVersion": "2016-08-01",
            "name": "web",
            "type": "sourcecontrols",
            "dependsOn": [
              "[resourceId('Microsoft.Web/Sites', variables('westWeb'))]"
            ],
            "properties": {
              "RepoUrl": "https://github.com/danielscholl/demo-appgw-path-code.git",
              "branch": "master",
              "IsManualIntegration": true
            }
          },
          {
            "apiVersion": "2016-08-01",
            "name": "logs",
            "type": "config",
            "dependsOn": [
                "[resourceId('Microsoft.Web/Sites', variables('westWeb'))]"
            ],
            "properties": {
              "httpLogs": {
                "fileSystem": {
                  "retentionInMb": 100,
                  "retentionInDays": 90,
                  "enabled": true
                }
              }
            }
          }
        ]
      },
      {
